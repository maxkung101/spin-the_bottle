<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.rccc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>RCCC Small Group</name>
    <description>
  Attention caretakers and counselors, you can use this app to lead your small group. You can also randomly choose which person in your small group to start the bible reading, or close in prayer.        <br />
</description>
    <author email="maxkung06@gmail.com" href="">Max Kung</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/logo.png" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="permissions" value="none" />
    <preference name="target-device" value="universal" />
</widget>
